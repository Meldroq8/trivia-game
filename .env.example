# Firebase Configuration (Vite Environment Variables)
# Copy this file to .env and fill in your actual values
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# CloudFront CDN Configuration
VITE_CLOUDFRONT_DOMAIN=your_cloudfront_domain.cloudfront.net
VITE_CLOUDFRONT_ENABLED=true
VITE_CDN_BASE_URL=https://your_cloudfront_domain.cloudfront.net

# AWS S3 Configuration (for admin uploads)
VITE_AWS_REGION=your_aws_region
VITE_AWS_S3_BUCKET=your_s3_bucket_name
VITE_AWS_ACCESS_KEY_ID=your_aws_access_key
VITE_AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# Optional: GitHub token for manual deployments
VITE_GITHUB_TOKEN=your_github_token_here